package mmx;

class Transaction {
	
	hash_t id;
	
	uint version;
	
	vector<tx_in_t> inputs;
	
	vector<tx_out_t> outputs;
	
	vector<Operation*> execute;
	
	vector<Solution*> solutions;
	
	
	void finalize();
	
	bool is_valid() const;
	
	hash_t calc_hash() const;
	
	Solution* get_solution(uint index) const;
	
	ulong calc_min_fee(ChainParams* params) const;
	
}
