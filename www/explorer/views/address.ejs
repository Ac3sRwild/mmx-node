
<h4>Address | <span class="font-monospace"><%= address %></span></h4>

<h5>Balance<b class="font-monospace"> <%= balance %> </b>MMX</h5>

<p>
<table class="table">
<thead>
<tr>
<th>Height</th>
<th>Type</th>
<th>Amount</th>
<th>Address</th>
<th>TX ID</th>
</tr>
</thead>

<tbody class="font-monospace">
<% history.forEach(function(entry) { %>
<tr>
<td><%= entry.height %></td>
<td><%= entry.type %></td>
<td><%= entry.amount %></td>
<td><a href="address?addr=<%= entry.address %>"><%= entry.address %></a></td>
<td><a href="transaction?id=<%= entry.txid %>"><%= entry.txid %></a></td>
</tr>
<% }); %>
</tbody>
</table>
</p>
